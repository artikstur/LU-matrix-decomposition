<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.Resources.Themes.Dark">

    <Color x:Key="MyPrimary">#222831</Color>
    <Color x:Key="MySecondary">#3bceac</Color>
    <Color x:Key="MyTertiary">#540d6e</Color>
    <Color x:Key="TextColor">#fff</Color>
    <Color x:Key="ChangeTheme">#4CCD99</Color>

    <SolidColorBrush x:Key="MyPrimaryBrush" Color="{StaticResource MyPrimary}"/>
    <SolidColorBrush x:Key="MySecondaryBrush" Color="{StaticResource MySecondary}"/>
    <SolidColorBrush x:Key="MyTertiaryBrush" Color="{StaticResource MyTertiary}"/>
</ResourceDictionary>